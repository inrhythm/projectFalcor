{
  "name": "projectfalkor",
  "version": "0.1.0",
  "private": true,
  "env": "production",
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "node-sass": "^4.0.0",
    "normalize.css": "^8.0.0",
    "postcss": "^6.0.0",
    "postcss-assets": "^5.0.0",
    "postcss-cli": "^5.0.0",
    "postcss-cssnext": "^3.0.0",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-import": "^11.0.0",
    "postcss-pxtorem": "^4.0.0",
    "css-mqpacker": "^6.0.0",
    "cssnano": "^4.0.0-rc.2",
    "npm-run-all": "^4.0.0",
    "onchange": "^3.0.0"
  },
  "scripts": {
    "css": "npm-run-all css:sass css:postcss",
    "css:sass": "node-sass --output-style='nested' --sourcemap=none ./src/styles/main.scss -o ./public/css/",
    "css:postcss": "postcss ./public/css/*.css --replace --config ./postcss.config.js",
    "watch:css": "onchange './src/styles/**/*.scss' -v -i -- npm run css",
    "falkor:start": "npm run start & npm run watch:css",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
